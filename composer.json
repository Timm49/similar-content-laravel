{
    "name": "timm49/laravel-similar-content",
    "description": "A Laravel package for finding similar content",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Timm49\\LaravelSimilarContent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Timm49\\LaravelSimilarContent\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^8.0",
        "laravel/pint": "^1.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 